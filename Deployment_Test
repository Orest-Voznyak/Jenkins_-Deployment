echo "=============Build Started============"
cat <<EOF > index.html
<html>
<body bgcolor=black>
<center>
<h2><font color=yellow>Hello from Voznyk Orest</font></h2>

<font color=blue>orest-25@ukr.net</font>

</center>
</body>
</html>
EOF
echo "=============Build Finished============"

echo "=============Test Started============"
result='grep "HÐµllo" index.html | wc -1'
echo $result
if [ "$result" = "1" ]
then
	echo "Test Passed"
else
	echo "Test Failed"
    exit 1
fi
echo "=============Test Finished============"
